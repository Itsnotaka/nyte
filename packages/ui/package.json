{
  "name": "@nyte/ui",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    "./components/*": {
      "types": "./src/components/ui/*.tsx",
      "default": "./src/components/ui/*.tsx"
    },
    "./lib/*": {
      "types": "./src/lib/*.ts",
      "default": "./src/lib/*.ts"
    },
    "./hooks/*": {
      "types": "./src/hooks/*.ts",
      "default": "./src/hooks/*.ts"
    }
  },
  "scripts": {
    "typecheck": "tsgo --noEmit",
    "clean": "rm -rf .turbo .cache node_modules dist"
  },
  "dependencies": {
    "@base-ui/react": "^1.2.0",
    "@central-icons-react/round-filled-radius-2-stroke-1.5": "^1.1.142",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "motion": "^12.34.3",
    "next-themes": "^0.4.6",
    "react": "^19.2.4",
    "react-day-picker": "^9.13.2",
    "react-dom": "^19.2.4",
    "react-resizable-panels": "^4.6.5",
    "recharts": "2.15.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.5.0"
  },
  "devDependencies": {
    "@nyte/tsconfig": "workspace:*",
    "@types/node": "^25.3.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "tailwindcss": "^4.2.1",
    "typescript": "6.0.0-beta"
  }
}
